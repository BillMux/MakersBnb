<h1> Booked Dates </h1>

<% @bookings.each do |booking| %>
  <sub> <%= booking.start_date %> </sub>
  <sub> <%= booking.end_date %> </sub>
<% end %>


<form action="/<%= params[:id] %>/booking" method="post" >
  <input type="date" id="start_date" placeholder="2018/06/23">
  <input type="date" id="end_date" placeholder="2018/06/29">
  <input type="Submit" value="Submit">
</form>
